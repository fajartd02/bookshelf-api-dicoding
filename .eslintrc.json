{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": "error",
        "indent": ["error", 4],
        "no-unused-vars": "warn",
        "linebreak-style": ["error", "unix"],
        "no-console": "off"
    }
}
